{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "expo export --platform web",
    "build:web": "npx vite build",
    "build:dev": "npx vite build --mode development",
    "preview": "npx vite preview",
    "analyze": "node scripts/build-analyze.js",
    "qa": "node scripts/qa-check.js",
    "monitor:tenant": "node scripts/tenant-integrity-scan.cjs",
    "ci:verify-env": "node scripts/verify-ci-env.cjs",
    "migrate": "node scripts/migrate.js",
    "db:generate": "drizzle-kit generate:pg --schema=./data/schema/pg.ts",
    "db:push": "drizzle-kit push:pg --schema=./data/schema/pg.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "collapse:migrations": "node scripts/collapse-migrations.cjs --stamp 20251015",
    "clean:headers": "tsx scripts/auto-clean-headers.ts",
    "clean:headers:dry": "tsx scripts/auto-clean-headers.ts --dry-run",
    "clean:unused": "tsx scripts/purge-unused.ts",
    "clean:unused:dry": "tsx scripts/purge-unused.ts --dry-run",
    "migrate:force-shell": "tsx scripts/force-unified-shell.ts",
    "migrate:force-shell:dry": "tsx scripts/force-unified-shell.ts --dry-run",
    "migrate:full": "npm run clean:headers && npm run clean:unused && npm run migrate:force-shell"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tonconnect/ui-react": "^2.0.9",
    "axios": "^1.13.2",
    "lucide-react": "^0.300.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^6.30.2",
    "zod": "^3.22.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/environment-jsdom": "file:packages/vitest-environment-jsdom",
    "baseline-browser-mapping": "^2.9.4",
    "expo": "^50.0.0",
    "jsdom": "^27.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supabase": "^2.48.3",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^4.4.0",
    "vitest": "^3.2.4"
  }
}
