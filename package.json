{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "expo export --platform web",
    "build:web": "npx vite build",
    "preview": "npx vite preview",
    "analyze": "node scripts/build-analyze.js",
    "qa": "node scripts/qa-check.js",
    "migrate": "node scripts/migrate.js",
    "db:generate": "drizzle-kit generate:pg --schema=./data/schema/pg.ts",
    "db:push": "drizzle-kit push:pg --schema=./data/schema/pg.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "@twa-dev/sdk": "^8.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "expo": "^50.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.44.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  }
}
