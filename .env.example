# Frontend (VITE_)
# Set to 1 to use Space and Time instead of Supabase (leave empty/0 for Supabase)
VITE_USE_SXT=
VITE_SXT_ENDPOINT=https://api.spaceandtime.dev/v1/sql
VITE_SXT_API_KEY=
VITE_SXT_ENCRYPTION_KEY=
VITE_SXT_ENCRYPTION_SALT=

# Optional integrations
VITE_TELEGRAM_BOT_USERNAME=

# Log Level Configuration (DEBUG, INFO, WARN, ERROR, NONE)
VITE_LOG_LEVEL=ERROR

# Web3 Configuration
VITE_ENABLE_WEB3_AUTH=true
VITE_ENABLE_ETHEREUM_AUTH=true
VITE_ENABLE_SOLANA_AUTH=true
VITE_ENABLE_TON_AUTH=true
VITE_ENABLE_WALLET_AUTH=false

